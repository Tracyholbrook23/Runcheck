{
  "expo": {
    "name": "runcheck-new",
    "slug": "runcheck-new",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo/runcheck-logo-full.png",
